    <!-- http://purl.obolibrary.org/obo/PO_0009010 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PO_0009010">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PO_0009007"/>
        <obo:IAO_0000115 xml:lang="en">A multi-tissue plant structure (PO:0025496) that develops from a plant ovule (PO:0020003) and has as parts a plant embryo (PO:0009009) enclosed in a seed coat (PO:0009088).</obo:IAO_0000115>
        <obo:IAO_0000118 xml:lang="en">pyrene
diaspore
semilla
seed tissue</obo:IAO_0000118>
